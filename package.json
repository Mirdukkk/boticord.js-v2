{
  "name": "boticord.js",
  "version": "2.0.4",
  "description": "An API wrapper for https://boticord.top/ written in TS for JS/TS",
  "main": "dist/index.js",
  "module": "dist/index.js",
  "types": "types/index.d.ts",
  "scripts": {
    "prepublishOnly": "npm run build",
    "docs": "npm install && npm install discordoo eris discord.js --no-save && node ./typedoc.js",
    "build": "ttsc"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/san4ern/boticord.js.git"
  },
  "keywords": [
    "boticord",
    "js",
    "javascript",
    "discord"
  ],
  "author": "san4ouZ",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/boticord/boticord.js/issues"
  },
  "homepage": "https://github.com/boticord/boticord.js#readme",
  "dependencies": {
    "fastify": "^3.27.1",
    "tiny-typed-emitter": "^2.1.0",
    "tslib": "^2.3.1",
    "undici": "^5.10.0"
  },
  "devDependencies": {
    "@zerollup/ts-transform-paths": "^1.7.18",
    "discord.js": "^14.6.0",
    "discordoo": "^0.11.8",
    "eris": "^0.17.1",
    "ttypescript": "^1.5.12",
    "typedoc": "^0.22.11",
    "typescript": "^4.6.4"
  },
  "peerDependencies": {
    "discord.js": "^14.3.0",
    "discordoo": "^0.11.3",
    "eris": "^0.15.1"
  },
  "peerDependenciesMeta": {
    "discord.js": {
      "optional": true
    },
    "discordoo": {
      "optional": true
    },
    "eris": {
